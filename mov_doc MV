select 
        pd.cd_protocolo_doc
       ,paciente.nm_paciente
       ,pd.cd_setor
       ,sto.nm_setor setor_origem
       ,pd.nm_usuario_envio
       --,to_char(dt_envio,'dd/mm/yyyy hh24:mi') dt_envio
       ,dt_envio
       ,pd.cd_setor_destino
       ,std.nm_setor setor_destino
       ,ipd.cd_atendimento
       ,ipd.cd_reg_fat
       ,reg_fat.dt_inicio
       ,reg_fat.dt_final
       ,sum(reg_fat.vl_total_conta) vl_total_conta
       ,ipd.dt_realizacao
       ,to_char(ipd.hr_realizacao, 'dd/mm/yyyy hh24:mi') hora_realizacao
       ,dt_recebimento
       ,nm_usuario_recebimento
       ,dpt.ds_documento_prot
       ,convenio.nm_convenio
       ,unid_int.ds_unid_int
       ,sta.nm_setor
        
from 
     protocolo_doc pd
    ,it_protocolo_doc ipd
    ,documento_prot dpt
    ,setor sto
    ,setor std
    ,reg_fat
    ,convenio
    ,atendime
    ,paciente
    ,leito
    ,unid_int
    ,setor sta
where
    ipd.cd_protocolo_doc      =   pd.cd_protocolo_doc
and dpt.cd_documento_prot     =   ipd.cd_documento_prot   
and sto.cd_setor              =   pd.cd_setor
and std.cd_setor              =   pd.cd_setor_destino
and reg_fat.cd_reg_fat        =   ipd.cd_reg_fat
and convenio.cd_convenio      =   reg_fat.cd_convenio
and atendime.cd_atendimento   =   ipd.cd_atendimento
and paciente.cd_paciente      =   atendime.cd_paciente
and leito.cd_leito            =   atendime.cd_leito
and unid_int.cd_unid_int      =   leito.cd_unid_int
and sta.cd_setor              =   unid_int.cd_setor
and reg_fat.sn_fechada        =   'N'
and dpt.cd_documento_prot in (29)
and atendime.tp_atendimento   = 'I'
and pd.dt_envio           =  (select dt_envio2
                                from
                                (
                                  select max(p.dt_envio) dt_envio2
                                  from protocolo_doc p, it_protocolo_doc i
                                  where
                                      i.cd_protocolo_doc  = p.cd_protocolo_doc
                                  and i.cd_atendimento    = ipd.cd_atendimento
                                  and i.cd_reg_fat        = ipd.cd_reg_fat
                                )
                              where 1 = 1
                              --order by dt_envio2 desc
                              )

--and  trunc(dt_atendimento) between to_date('01/10/2018','dd/mm/yyyy') and to_date('23/10/2018','dd/mm/yyyy')
group by
        pd.cd_protocolo_doc
       ,paciente.nm_paciente
       ,pd.cd_setor
       ,sto.nm_setor
       ,pd.nm_usuario_envio
       ,dt_envio
       ,pd.cd_setor_destino
       ,std.nm_setor
       ,ipd.cd_atendimento
       ,ipd.cd_reg_fat
       ,reg_fat.dt_inicio
       ,reg_fat.dt_final
       ,ipd.dt_realizacao
       ,to_char(ipd.hr_realizacao, 'dd/mm/yyyy hh24:mi')
       ,dt_recebimento
       ,nm_usuario_recebimento
       ,dpt.ds_documento_prot
       ,convenio.nm_convenio
       ,unid_int.ds_unid_int
       ,sta.nm_setor

order by 
        nm_paciente, dt_envio asc

;


